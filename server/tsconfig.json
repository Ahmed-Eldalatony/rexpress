{
  "type": "CommonJs",
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "NodeNext",
    "target": "ESNext",
    "esModuleInterop": true,
    "rootDir": ".",
    "strict": false,
    "baseUrl": ".",
    "paths": {
      "*": [
        "node_modules/*"
      ],
    },
    "noEmit": true,
    "allowSyntheticDefaultImports": true
  }
}
